{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node"
      },
      {
        "src": "dist/**",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/api/usuarios/(.*)",
        "dest": "/api/usuarios/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/dist/$1"
      }
    ]
  }
  